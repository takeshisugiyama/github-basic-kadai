        "  def __init__(self, name=\"\", age=0):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def print_info(self):\n",
        "    print(f\"名前: {self.name}, 年齢:{self.age}\")\n",
        "\n",
        "Potato = Human(\"ポテトさん\", 35)\n",
        "\n",
        "Potato.print_info()\n",
        "\n",
        "\n"
